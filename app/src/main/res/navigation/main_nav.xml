<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="be.ehb.jokerapp.fragments.StartFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_start">
        <action
            android:id="@+id/start_to_jokelist"
            app:destination="@id/jokesListFragment" />
    </fragment>
    <fragment
        android:id="@+id/jokesListFragment"
        android:name="be.ehb.jokerapp.fragments.JokesListFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_jokes_list" />
</navigation>